{
  "name": "biopocket-backend",
  "version": "0.2.1",
  "private": true,
  "scripts": {
    "anonymize": "node scripts/anonymize.js",
    "create-admin": "node scripts/create-admin.js",
    "dev": "concurrently \"npm run dev:docs\" \"npm run dev:server\"",
    "dev:all": "concurrently \"npm run dev:docs\" \"npm run dev:server\" \"npm run dev:test\"",
    "dev:docs": "npm run docs && concurrently \"npm run docs:serve\" \"npm run docs:watch\"",
    "dev:lintAndTest": "npm run lint && npm test",
    "dev:lintAndTest:watch": "onchange \"config/**/*.js\" knexfile.js \"scripts/**/*.js\" \"server/**/*.js\" \"utils/**/*.js\" -- npm run lint && npm test",
    "dev:server": "nodemon",
    "dev:test": "npm run dev:lintAndTest && npm run dev:lintAndTest:watch",
    "docs": "npm run docs:clean && concurrently \"npm run docs:api\" \"npm run docs:database\" \"npm run docs:src\" \"npm test\"",
    "docs:api": "mkdirp docs/api && raml2html -i server/api/index.raml -o docs/api/index.html",
    "docs:api:clean": "rimraf docs/api",
    "docs:api:watch": "onchange \"server/**/*.raml\" -- npm run docs:api",
    "docs:clean": "concurrently \"npm run docs:api:clean\" \"npm run docs:src:clean\"",
    "docs:database": "node scripts/embed-database-diagram.js",
    "docs:publish": "npm run docs && gh-pages -d docs",
    "docs:serve": "cross-env LOG_LEVEL=WARN node scripts/serve-docs.js",
    "docs:src": "jsdoc -c jsdoc.json",
    "docs:src:clean": "rimraf docs/src",
    "docs:src:watch": "onchange \"scripts/**/*.js\" \"server/**/*.js\" \"utils/**/*.js\" -- npm run docs:src",
    "docs:watch": "concurrently \"npm run docs:api:watch\" \"npm run docs:src:watch\"",
    "doctoc": "doctoc --notitle --github README.md DEVELOPMENT.md",
    "lint": "eslint config knexfile.js migrations scripts server utils",
    "lint:watch": "onchange \"config/**/*.js\" knexfile.js \"scripts/**/*.js\" \"server/**/*.js\" \"utils/**/*.js\" -- npm run lint",
    "migrate": "knex migrate:latest",
    "migrate:make": "knex migrate:make",
    "migrate:rollback": "knex migrate:rollback",
    "migrate:test": "cross-env NODE_ENV=test knex migrate:latest",
    "migrate:test:rollback": "cross-env NODE_ENV=test knex migrate:rollback",
    "sample-data": "node scripts/sample-data.js",
    "start": "node ./bin/www",
    "start:e2e": "cross-env BCRYPT_COST=1 DEFAULT_PAGINATION_LIMIT=5 CORS=true NODE_ENV=test SESSION_SECRET=secret npm run dev:server",
    "sync": "node scripts/sync-interface-data.js",
    "test": "cross-env NODE_ENV=test BCRYPT_COST=1 DEFAULT_PAGINATION_LIMIT=3 LOG_LEVEL=WARN nyc --reporter=html mocha --reporter=mocha-api-errors \"server/**/*.spec.js\"",
    "test:coveralls": "nyc report --reporter=text-lcov | coveralls",
    "test:debug": "cross-env NODE_ENV=test BCRYPT_COST=1 DEFAULT_PAGINATION_LIMIT=3 LOG_LEVEL=TRACE nyc --reporter=html --reporter=text mocha --reporter=mocha-api-errors \"server/**/*.spec.js\"",
    "test:watch": "onchange \"config/**/*.js\" \"server/**/*.js\" -- npm test"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "bookshelf": "^0.13.3",
    "bookshelf-geojson": "^1.0.0",
    "bookshelf-touch": "^1.1.4",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-jwt-policies": "^2.0.1",
    "express-serializer": "^1.0.1",
    "glob": "^7.1.2",
    "inflection": "^1.12.0",
    "jsonwebtoken": "^8.2.1",
    "knex": "^0.14.6",
    "knex-postgis": "^0.6.0",
    "lodash": "^4.17.10",
    "log4js": "^2.6.1",
    "moment": "^2.22.1",
    "orm-query-builder": "^1.0.0",
    "pg": "^7.4.3",
    "valdsl": "^0.4.6",
    "valib": "^2.0.0"
  },
  "devDependencies": {
    "babel-plugin-istanbul": "^4.1.6",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-iso8601": "^1.0.0",
    "chai-moment": "^0.1.0",
    "chai-objects": "^1.0.0",
    "chai-string": "^1.5.0",
    "chalk": "^2.4.1",
    "chance": "^1.0.16",
    "concurrently": "^3.5.1",
    "coveralls": "^3.0.1",
    "cross-env": "^5.1.5",
    "docdash": "^0.4.0",
    "doctoc": "^1.3.1",
    "enrich-api-error": "^1.0.2",
    "eslint": "^4.19.1",
    "eslint-plugin-sort-destructure-keys": "^1.0.4",
    "eslint-plugin-sort-requires-by-path": "^1.0.2",
    "fs-extra": "^6.0.1",
    "get-port": "^3.2.0",
    "gh-pages": "^1.1.0",
    "handlebars": "^4.7.7",
    "jsdoc": "^3.5.5",
    "live-server": "^1.2.0",
    "mkdirp": "^0.5.1",
    "mocha": "^5.1.1",
    "mocha-api-errors": "^1.0.2",
    "nodemon": "^1.17.4",
    "nyc": "^11.8.0",
    "onchange": "^4.0.0",
    "raml2html": "^7.1.0",
    "rimraf": "^2.6.2",
    "sinon": "^5.0.7",
    "sinon-chai": "^3.0.0",
    "superrest": "^1.0.0",
    "supertest": "^3.1.0",
    "test-value-generator": "^1.0.1"
  },
  "engines": {
    "node": ">= 10"
  }
}
